		{{ page.header }}
	</div>
</header>

<main class="page-content" role="main">
  <div class="visually-hidden">
    <a id="main-content" tabindex="-1"></a>
  </div>
	<div class="container mx-auto">
    {{ page.content }}
  </div>
</main>

<footer class="page-footer pt-4 pb-4 border-t-200 border-t-4">
	<div class="container mx-auto">
    {{ page.footer }}
  </div>
</footer>
